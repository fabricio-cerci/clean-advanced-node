    "eslint-plugin-promise": "^5.2.0",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "jest-mock-extended": "^2.0.4",
    "lint-staged": "^12.3.5",
    "pg-mem": "^1.9.11",
    "supertest": "^6.2.2",
    "ts-jest": "^27.1.3",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.6.2"
