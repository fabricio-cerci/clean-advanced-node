  "devDependencies": {
    "@jest-mock/express": "^1.4.5",
    "@types/axios": "^0.14.0",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.1",
    "@types/module-alias": "^2.0.1",
    "@types/node": "^17.0.21",
    "@types/supertest": "^2.0.12",
